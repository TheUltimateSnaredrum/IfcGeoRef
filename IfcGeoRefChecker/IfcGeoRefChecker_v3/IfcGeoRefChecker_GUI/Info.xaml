<Window x:Class="IfcGeoRefChecker_GUI.Info"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IfcGeoRefChecker_GUI"
        mc:Ignorable="d"
        Title="Info" Height="400" Width="600" Icon="img/icon_img.png">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="349" Margin="15,0,0,0" VerticalAlignment="Top" Width="560">
            <Label Content="Info" FontWeight="Bold" FontSize="14" />
            <TextBox  Name="block_info" TextWrapping="Wrap" Text="TextBlock" />
            <Label Content="Terms of Use" FontWeight="Bold" FontSize="14" />
            <TextBox Name="block_terms" Height="150" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible" Text="TextBox" />
        </StackPanel>
    </Grid>
</Window>
